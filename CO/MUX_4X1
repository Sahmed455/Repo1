#! /usr/bin/vvp
:ivl_version "10.1 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x6c3230 .scope module, "mux_4X1_Test" "mux_4X1_Test" 2 6;
 .timescale -9 -12;
v0x6e3ad0_0 .var "a", 3 0;
v0x6e3bb0_0 .var "b", 3 0;
v0x6e3c80_0 .var "c", 3 0;
v0x6e3d80_0 .var "d", 3 0;
v0x6e3e50_0 .var/i "i", 31 0;
v0x6e3f40_0 .net "out", 3 0, v0x6e3800_0;  1 drivers
v0x6e4000_0 .var "sel", 1 0;
S_0x6c33b0 .scope module, "uut" "mux_4X1" 2 21, 3 1 0, S_0x6c3230;
 .timescale -9 -12;
    .port_info 0 /INPUT 4 "a"
    .port_info 1 /INPUT 4 "b"
    .port_info 2 /INPUT 4 "c"
    .port_info 3 /INPUT 4 "d"
    .port_info 4 /INPUT 2 "sel"
    .port_info 5 /OUTPUT 4 "out"
v0x6c3620_0 .net "a", 3 0, v0x6e3ad0_0;  1 drivers
v0x6e3580_0 .net "b", 3 0, v0x6e3bb0_0;  1 drivers
v0x6e3660_0 .net "c", 3 0, v0x6e3c80_0;  1 drivers
v0x6e3720_0 .net "d", 3 0, v0x6e3d80_0;  1 drivers
v0x6e3800_0 .var "out", 3 0;
v0x6e3930_0 .net "sel", 1 0, v0x6e4000_0;  1 drivers
E_0x6b7f30/0 .event edge, v0x6e3930_0, v0x6e3720_0, v0x6e3660_0, v0x6e3580_0;
E_0x6b7f30/1 .event edge, v0x6c3620_0;
E_0x6b7f30 .event/or E_0x6b7f30/0, E_0x6b7f30/1;
    .scope S_0x6c33b0;
T_0 ;
    %wait E_0x6b7f30;
    %load/vec4 v0x6e3930_0;
    %dup/vec4;
    %pushi/vec4 0, 0, 2;
    %cmp/u;
    %jmp/1 T_0.0, 6;
    %dup/vec4;
    %pushi/vec4 1, 0, 2;
    %cmp/u;
    %jmp/1 T_0.1, 6;
    %dup/vec4;
    %pushi/vec4 2, 0, 2;
    %cmp/u;
    %jmp/1 T_0.2, 6;
    %dup/vec4;
    %pushi/vec4 3, 0, 2;
    %cmp/u;
    %jmp/1 T_0.3, 6;
    %jmp T_0.4;
T_0.0 ;
    %load/vec4 v0x6c3620_0;
    %assign/vec4 v0x6e3800_0, 0;
    %jmp T_0.4;
T_0.1 ;
    %load/vec4 v0x6e3580_0;
    %assign/vec4 v0x6e3800_0, 0;
    %jmp T_0.4;
T_0.2 ;
    %load/vec4 v0x6e3660_0;
    %assign/vec4 v0x6e3800_0, 0;
    %jmp T_0.4;
T_0.3 ;
    %load/vec4 v0x6e3720_0;
    %assign/vec4 v0x6e3800_0, 0;
    %jmp T_0.4;
T_0.4 ;
    %pop/vec4 1;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x6c3230;
T_1 ;
    %vpi_call 2 33 "$dumpfile", "mux_4X1_Map.vcd" {0 0 0};
    %vpi_call 2 34 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x6c3230 {0 0 0};
    %vpi_call 2 36 "$monitor", "[%0t] sel=0x%0h a=0x%0h b=0x%0h c=0x%0h d=0x%0h out=0x%0h", $time, v0x6e4000_0, v0x6e3ad0_0, v0x6e3bb0_0, v0x6e3c80_0, v0x6e3d80_0, v0x6e3f40_0 {0 0 0};
    %pushi/vec4 0, 0, 2;
    %assign/vec4 v0x6e4000_0, 0;
    %vpi_func 2 40 "$random" 32 {0 0 0};
    %pad/s 4;
    %assign/vec4 v0x6e3ad0_0, 0;
    %vpi_func 2 41 "$random" 32 {0 0 0};
    %pad/s 4;
    %assign/vec4 v0x6e3bb0_0, 0;
    %vpi_func 2 42 "$random" 32 {0 0 0};
    %pad/s 4;
    %assign/vec4 v0x6e3c80_0, 0;
    %vpi_func 2 43 "$random" 32 {0 0 0};
    %pad/s 4;
    %assign/vec4 v0x6e3d80_0, 0;
    %pushi/vec4 1, 0, 32;
    %store/vec4 v0x6e3e50_0, 0, 32;
T_1.0 ;
    %load/vec4 v0x6e3e50_0;
    %cmpi/s 4, 0, 32;
    %jmp/0xz T_1.1, 5;
    %delay 100000, 0;
    %load/vec4 v0x6e3e50_0;
    %pad/s 2;
    %assign/vec4 v0x6e4000_0, 0;
    %load/vec4 v0x6e3e50_0;
    %addi 1, 0, 32;
    %store/vec4 v0x6e3e50_0, 0, 32;
    %jmp T_1.0;
T_1.1 ;
    %delay 100000, 0;
    %vpi_call 2 51 "$finish" {0 0 0};
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "mux_4X1_Test.v";
    "./mux_4X1.v";
